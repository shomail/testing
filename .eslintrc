{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": "2018"
  },
  "extends": ["eslint:recommended", "eslint-config-prettier"],
  "rules": {
    "valid-typeof": "warn",
    "no-console": "off",
    "no-unused-vars": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  }
}
